{
  "version": 2,
  "builds": [
    {
      "src": "build/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/client/**",
      "use": "@vercel/static",
      "config": { "maxAge": 3600 }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/build/client/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "build/server/index.js"
    }
  ]
}